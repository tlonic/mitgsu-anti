jQuery(document).ready((function($){$(".hamburger-menu").click((function(){$(this).toggleClass("open"),$(".primary-nav").toggle()})),$(".accordion .row:first").addClass("active"),$(".accordion .row").click((function(){$(".text").slideUp(),$(".row").removeClass("active"),$(this).hasClass("active")?$(".text").stop():$(this).addClass("active").find(".text").slideDown()})),$("#answer").click((function(){$(".modal").addClass("active"),$("body").addClass("fixed")})),$(".close-modal").click((function(){$(".modal").removeClass("active"),$("body").removeClass("fixed")})),$("body").removeClass("loading-js")}));