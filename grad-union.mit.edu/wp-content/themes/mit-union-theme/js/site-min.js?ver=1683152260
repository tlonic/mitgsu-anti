jQuery(document).ready((function($){$(".hamburger-menu").click((function(){$(this).toggleClass("open"),$(".primary-nav").toggle()})),$(".accordion .row:first").addClass("active"),$(".accordion .row:first .text").show(),$(".accordion .row").click((function(){$(this).toggleClass("active"),$(this).find("div.text").slideToggle()})),$(".show-all").click((function(){$(".row").addClass("active"),$("div.text").slideDown(),$(this).hide(),$(".close-all").show()})),$(".close-all").click((function(){$(".row").removeClass("active"),$("div.text").slideUp(),$(this).hide(),$(".show-all").show()})),$("#answer").click((function(){$(".modal").addClass("active"),$("body").addClass("fixed")})),$(".close-modal").click((function(){$(".modal").removeClass("active"),$("body").removeClass("fixed")})),$("body").removeClass("loading-js")}));